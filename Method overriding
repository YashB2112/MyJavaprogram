class Vehicle {
    public void displayInfo() {
        System.out.println("This is a vehicle.");
    }
}
class Car extends Vehicle {
    @Override
    public void displayInfo() {
        System.out.println("This is a car.");
    }
}
public class Main {
    public static void main(String[] args) {
        Vehicle vehicle = new Vehicle();
        vehicle.displayInfo();
        Vehicle car = new Car();
        car.displayInfo();
    }
}
